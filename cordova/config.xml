<?xml version='1.0' encoding='utf-8'?>
<widget id="ishigo1987.tabristest" version="0.1.0">
  <name>Tabris Test</name>
  <description>
    Example Tabris.js App
  </description>
  <author email="stephnguets@yahoo.fr">
    ishigo1987
  </author>
  <preference name="EnableDeveloperConsole" value="$IS_DEBUG" />
  <preference name="AndroidExtraFilesystems" value="files,files-external,documents,sdcard,cache,cache-external,assets,root" />
  <preference name="iosExtraFilesystems" value="library,library-nosync,documents,documents-nosync,cache,bundle,root" />
  <plugin name="cordova-plugin-contacts-phonenumbers" spec="0.0.12" />
  <plugin name="cordova-plugin-android-permissions" spec="1.0.2" />
  <plugin name="tabris-plugin-lottie" spec="https://github.com/eclipsesource/tabris-plugin-lottie.git" />
  <plugin name="cordova-plugin-x-toast" spec="2.7.2" />
  <plugin name="cordova-plugin-locationservices" spec="2.1.0" />
  <plugin name="cordova-plugin-request-location-accuracy" spec="https://github.com/dpa99c/cordova-plugin-request-location-accuracy.git" />
  <plugin name="cordova-plugin-opensettings" spec="1.3.0" />
  <plugin name="cordova-plugin-sim" spec="https://github.com/pbakondy/cordova-plugin-sim.git" />
</widget>
