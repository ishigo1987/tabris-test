<?xml version='1.0' encoding='utf-8'?>
<widget id="ishigo1987.tabristest" version="0.1.0" xmlns:android="http://schemas.android.com/apk/res/android">
  <name>Tabris Test</name>
  <description>
    Example Tabris.js App
  </description>
  <author email="stephnguets@yahoo.fr">
    ishigo1987
  </author>
  <preference name="EnableDeveloperConsole" value="$IS_DEBUG" />
  <preference name="AndroidExtraFilesystems" value="files,files-external,documents,sdcard,cache,cache-external,assets,root" />
  <preference name="iosExtraFilesystems" value="library,library-nosync,documents,documents-nosync,cache,bundle,root" />
  <plugin name="cordova-plugin-contacts" spec="https://github.com/apache/cordova-plugin-contacts.git" />
  <plugin name="cordova-plugin-foreground-service" spec="1.1.2" />
  <plugin name="cordova-plugin-doze-Optimize" spec="https://github.com/thomas550i/cordova-plugin-doze-Optimize.git" />
  <plugin name="cordova-plugin-powermanagement" spec="https://github.com/boltex/cordova-plugin-powermanagement.git" />
  <plugin name="cordova-plugin-android-wifi-lock" spec="https://github.com/boedy/cordova-plugin-android-wifi-lock.git" />
  <plugin name="Cordova-SMS-Plugin" spec="https://github.com/rampicos/Cordova-SMS-Plugin.git" />
  <plugin name="cordova-plugin-background-service" spec="1.3.5" />
  <plugin name="cordova-plugin-foreground-service" spec="1.1.2" />
  <platform name="android">
  	<config-file target="app/src/main/AndroidManifest.xml" parent="/manifest">
  	  <uses-permission android:name="android.permission.READ_CONTACTS"/>
  	  <uses-permission android:name="android.permission.WRITE_CONTACTS"/>
  	  <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
    </config-file>
  </platform>
</widget>
